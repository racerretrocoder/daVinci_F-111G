        <int n="8" alias="/systems/electrical/outputs/nav-lights">  0 </int> <!-- 8 nav-lights -->
        <int n="9" alias="/sim/model/lights/beacon/state">  false </int> <!-- 9 strobe-lights -->
        <int n="10" alias="/controls/electric/key-pos">  0 </int> <!-- 10 key position -->
        <int n="11" type="int" alias="/surface-positions/left-flap-pos-norm"/>
        <int n="12" type="int" alias="/surface-positions/right-flap-pos-norm"/>
        <int n="15" type="int" alias="/sim/model/daVinci_SU-34/smokewinderB1"/>
        <int n="16" type="int" alias="/sim/model/daVinci_SU-34/smokewinderB9"/>
        <int n="17" type="int" alias="/sim/model/daVinci_SU-34/smokewinderW1"/>
        <int n="18" type="int" alias="/sim/model/daVinci_SU-34/smokewinderW9"/>
        <float n="1" alias="/velocities/airspeed-kt/"/>
        <float n="2" alias="/velocities/mach"/>
        <float n="3" alias="/gear/gear[2]/wow/"/>
        <float n="4" type="double" alias="/surface-positions/right-aileron-pos-norm"/>
        <float n="5" type="double" alias="controls/flight/elevator"/>
        <float n="6" type="double" alias="/surface-positions/right-taileron-pos-norm"/>
        <float n="7" type="double" alias="/gear/gear[0]/steering-norm"/>
        <float n="8" alias="/position/altitude-ft"> 0 </float><!-- altitude for Smoke/Trail -->
        <float n="9" type="double" alias="/instrumentation/doors/wings/position-norm"/>
        <float n="10" alias="/fdm/jsbsim/fcs/AIM120"/>
        <float n="11" alias="/surface-positions/flap-pos-norm"/>
        <float n="13" alias="/surface-positions/left-aileron-pos-norm"/>

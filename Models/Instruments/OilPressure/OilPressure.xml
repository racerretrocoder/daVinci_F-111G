<?xml version="1.0"?>

<PropertyList> 
<path>OilPressure.ac</path>
	
	<!-- oil pressure -->
	<animation>
		<name>oil-pressure-psi-l</name>
		<type>rotate</type>
		<object-name>needle-oil-l</object-name>
		<property>engines/engine[0]/oilp-norm</property>
		<interpolation>
			<entry><ind>0.0000</ind><dep>159</dep></entry><!--0-->
			<entry><ind>0.0214</ind><dep>265</dep></entry><!--iddle-->
			<entry><ind>0.1804</ind><dep>302</dep></entry><!--25% thrust-->
			<entry><ind>0.9870</ind><dep>386</dep></entry><!--max thrust-->
		</interpolation>
		<center>
			<y-m>-0.079</y-m>
			<z-m>-0.054</z-m>
		</center>
		<axis><x>-1</x></axis>
	</animation>

	<animation>
		<name>oil-pressure-psi-r</name>
		<type>rotate</type>
		<object-name>needle-oil-r</object-name>
		<property>engines/engine[1]/oilp-norm</property>
		<interpolation> 
			<entry><ind>0.0000</ind><dep>159</dep></entry><!--0-->
			<entry><ind>0.0214</ind><dep>265</dep></entry><!--iddle-->
			<entry><ind>0.1804</ind><dep>302</dep></entry><!--25% thrust-->
			<entry><ind>0.987</ind><dep>386</dep></entry><!--max thrust-->
		</interpolation>
		<center>
			<y-m>-0.020</y-m>
			<z-m>-0.054</z-m>
		</center>
		<axis>
		<x>-1</x></axis>
	</animation>


	<!-- lighting -->

	<animation> 
		<object-name>mount</object-name>
		<type>material</type>
		<condition><not><property>sim/rendering/rembrandt/enabled</property></not></condition>
		<emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.017</red>
      <green> 0.60 </green>
      <blue>  0.65 </blue>
    </emission>
	</animation>

	<animation>
		<object-name>face</object-name>
		<type>material</type>
		<condition><not><property>sim/rendering/rembrandt/enabled</property></not></condition>
		<emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.017</red>
      <green> 0.60 </green>
      <blue>  0.65 </blue>
    </emission>
	</animation>

	<animation>
		<object-name>needles</object-name>
		<type>material</type>
		<condition><not><property>sim/rendering/rembrandt/enabled</property></not></condition>
		<emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.017</red>
      <green> 0.60 </green>
      <blue>  0.65 </blue>
    </emission>
	</animation>

	
</PropertyList>

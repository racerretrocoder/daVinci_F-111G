<?xml version="1.0"?>
<PropertyList>

 <path>clock.ac</path>


 <animation>
  <type>material</type>
  <object-name>Hr.hand</object-name>
  <object-name>Mn.hand</object-name>
  <object-name>Sc.hand</object-name>
  <object-name>Tm.hand</object-name>
  <object-name>Clock.ring</object-name>
  <object-name>Clock.face</object-name>
  <emission>
       <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.017</red>
      <green> 0.60 </green>
      <blue>  0.65 </blue>
    </emission>
 </animation>

    <animation>
        <type>rotate</type>
        <object-name>Hr.hand</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
    <factor>0.008333</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Mn.hand</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
    <factor>0.1</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Sc.hand</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
    <factor>6</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Tm.hand</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
    <factor>0.4</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>-0.065</z>
        </axis>
        <center>
            <x-m> 0.0054</x-m>
            <y-m> 0.0</y-m>
            <z-m>-0.0164478</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Clock.ring</object-name>
        <property>instrumentation/clock/bezel-rotation-min</property>
    <factor>6</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

 </PropertyList>
